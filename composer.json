{
    "name": "andruby/deep-login",
    "description": "登录组件封装，适配uniapp、小程序后台接口",
    "license": "MIT",
    "authors": [
        {
            "name": "andruby"
        }
    ],
    "keywords": [
        "laravel",
        "auth",
        "tokens",
        "guard"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5|~6|~7",
        "illuminate/database": "~5|~6|~7",
        "illuminate/auth": "~5|~6|~7",
        "alibabacloud/sdk": "^1.8",
        "andruby/tokens-auth": "^0.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Andruby\\Login\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/Factories"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andruby\\Login\\LoginServiceProvider"
            ]
        }
    }
}
